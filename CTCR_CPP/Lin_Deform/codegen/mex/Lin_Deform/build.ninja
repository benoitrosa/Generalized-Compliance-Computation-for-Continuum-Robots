# CompilerName=clang
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/maci64
matlabdir = /Applications/MATLAB_R2023a.app
startdir = /Users/guillaume/Desktop/Seafile/THESE_LODS_COMPLET/PERSO/Generalized-Compliance-Computation-for-Continuum-Robots/CTCR_Shape_Functions

# Toolchain information
cc = /usr/bin/xcrun -sdk macosx13.3 clang
cxx = /usr/bin/xcrun -sdk macosx13.3 clang++
linker = /usr/bin/xcrun -sdk macosx13.3 clang
linkerxx = /usr/bin/xcrun -sdk macosx13.3 clang++

cflags =  -fno-common -arch x86_64 -mmacosx-version-min=10.15 -fexceptions -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX13.3.sdk -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -fvisibility=hidden -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=Lin_Deform_mex -DMW_NEEDS_VERSION_H -c 

cxxflags =  -fno-common -arch x86_64 -mmacosx-version-min=10.15 -fexceptions -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX13.3.sdk -fobjc-arc -std=c++14 -stdlib=libc++ -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -fvisibility=hidden -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=Lin_Deform_mex -DMW_NEEDS_VERSION_H -c 

ldflags =  -Wl,-twolevel_namespace -undefined error -arch x86_64 -mmacosx-version-min=10.15 -Wl,-syslibroot,/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX13.3.sdk -framework Cocoa -bundle  -stdlib=libc++ -L"$matlabdir/bin/maci64" -lmx -lmex -lmat -L"$matlabdir/extern/bin/maci64" -lMatlabDataArray -lMatlabEngine  -Wl,-rpath,@loader_path -o Lin_Deform_mex.mexmaci64 -lemlrt -lcovrt -lut -lmwmathutil 

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $linkobjs $ldflags

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/Lin_Deform_data.o : cxx $root/Lin_Deform_data.cpp
build $builddir/rt_nonfinite.o : cxx $root/rt_nonfinite.cpp
build $builddir/Lin_Deform_initialize.o : cxx $root/Lin_Deform_initialize.cpp
build $builddir/Lin_Deform_terminate.o : cxx $root/Lin_Deform_terminate.cpp
build $builddir/Lin_Deform.o : cxx $root/Lin_Deform.cpp
build $builddir/tic.o : cxx $root/tic.cpp
build $builddir/toc.o : cxx $root/toc.cpp
build $builddir/_coder_Lin_Deform_api.o : cxx $root/interface/_coder_Lin_Deform_api.cpp
build $builddir/_coder_Lin_Deform_mex.o : cxx $root/interface/_coder_Lin_Deform_mex.cpp
build $builddir/_coder_Lin_Deform_info.o : cxx $root/interface/_coder_Lin_Deform_info.cpp
build $builddir/cpp_mexapi_version.o : cxx $matlabdir/extern/version/cpp_mexapi_version.cpp

# Link
build $root/Lin_Deform_mex.mexmaci64 : linkxx $builddir/Lin_Deform_data.o $builddir/rt_nonfinite.o $builddir/Lin_Deform_initialize.o $builddir/Lin_Deform_terminate.o $builddir/Lin_Deform.o $builddir/tic.o $builddir/toc.o $builddir/_coder_Lin_Deform_api.o $builddir/_coder_Lin_Deform_mex.o $builddir/_coder_Lin_Deform_info.o $builddir/cpp_mexapi_version.o 
